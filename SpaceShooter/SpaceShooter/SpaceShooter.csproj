<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Sounds\music.mp3" />
    <None Remove="Sprites\enemy-big.png" />
    <None Remove="Sprites\enemy-medium.png" />
    <None Remove="Sprites\enemy-small.png" />
    <None Remove="Sprites\explosion.png" />
    <None Remove="Sprites\laser-bolts.png" />
    <None Remove="Sprites\power-up.png" />
    <None Remove="Sprites\Ship_1.png" />
    <None Remove="Sprites\Ship_3.png" />
    <None Remove="Sprites\Ship_5.png" />
  </ItemGroup>

  <ItemGroup>
    <Page Remove="view\App.xaml" />
  </ItemGroup>

  <ItemGroup>
    <ApplicationDefinition Include="view\App.xaml">
      <SubType>Designer</SubType>
    </ApplicationDefinition>
  </ItemGroup>

  <ItemGroup>
    <Content Include="Sounds\music.mp3" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="IUTGame">
      <HintPath>IUTGame.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Sprites\enemy-medium.png" />
    <Resource Include="Sprites\enemy-small.png" />
    <Resource Include="Sprites\explosion.png" />
    <Resource Include="Sprites\laser-bolts.png" />
    <Resource Include="Sprites\power-up.png" />
    <Resource Include="Sprites\Ship_1.png" />
    <Resource Include="Sprites\Ship_3.png" />
    <Resource Include="Sprites\Ship_5.png" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="model\Bonus\" />
  </ItemGroup>

</Project>
